<% content_for :body_classes, "search" %>
<% content_for :title, "Search" %>
<% content_for :extra_headers do %>
  <meta name="description"
        content="Search GOV.UK." />
<% end %>

<main id="content" role="main" class="search no-search-term">
  <form action="/search" method="get" accept-charset="utf-8" class="search-header js-live-search-form" role="search" autocomplete="off">
    <%= render 'search_field' %>
  </form>
</main>
